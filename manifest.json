{
  "id": "37f381e0",
  "name": "Pirate Gen",
  "version": "1.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "26.5"
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "pirate-gen-panel",
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 280,
        "height": 400
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 450
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/icon-dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark"],
          "species": ["generic"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/icon-light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"],
          "species": ["generic"]
        }
      ],
      "label": {
        "default": "Pirate Gen"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/plugin-icon.png",
      "scale": [1, 2],
      "theme": ["all"],
      "species": ["pluginList"]
    }
  ]
}